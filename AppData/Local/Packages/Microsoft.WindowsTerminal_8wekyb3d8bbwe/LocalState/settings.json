{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "copyFormatting": "none",
    "copyOnSelect": true,
    "focusFollowMouse": true,
    "launchMode": "focus",
    "snapToGridOnResize": false,
    "useAcrylicInTabRow": true,
    // Add custom actions and keybindings to this array.
    // To unbind a key combination from your defaults.json, set the command to "unbound".
    // To learn more about actions and keybindings, visit https://aka.ms/terminal-keybindings
    "actions":
    [
        // Unbind keys (disable default keybindings)
        { "command": null, "keys": "ctrl+shift+c" },    // Copy text
        { "command": null, "keys": "ctrl+shift+d" },    // Duplicate tab
        { "command": null, "keys": "ctrl+shift+f" },    // Find
        { "command": null, "keys": "ctrl+shift+n" },    // New window
        { "command": null, "keys": "ctrl+shift+p" },    // Toggle command palette
        { "command": null, "keys": "ctrl+shift+t" },    // New tab
        { "command": null, "keys": "ctrl+shift+v" },    // Paste
        { "command": null, "keys": "ctrl+shift+w" },    // Close pane
        { "command": null, "keys": "alt+enter" },       // Toggle full screen
        { "command": null, "keys": "alt+down" },        // Move focus down
        { "command": null, "keys": "alt+up" },          // Move focus up
        { "command": null, "keys": "alt+left" },        // Move focus left
        { "command": null, "keys": "alt+right" },       // Move focus right
        { "command": null, "keys": "alt+shift+down" },  // Resize pane down
        { "command": null, "keys": "alt+shift+up" },    // Resize pane up
        { "command": null, "keys": "alt+shift+left" },  // Resize pane left
        { "command": null, "keys": "alt+shift+right" }, // Resize pane right
        { "command": null, "keys": "ctrl+shift+down" }, // Scroll down
        { "command": null, "keys": "ctrl+shift+up" },   // Scroll up
        { "command": null, "keys": "ctrl+shift+pgdn" }, // Scroll down one page
        { "command": null, "keys": "ctrl+shift+pgup" }, // Scroll up one page
        { "command": null, "keys": "ctrl+shift+home" }, // Scroll to the bottom of history
        { "command": null, "keys": "ctrl+shift+end" },  // Scroll to the top of history
        { "command": null, "keys": "ctrl+alt+left" },   // Move focus to the last used pane
        { "command": null, "keys": "ctrl+shift+1" },    // New tab, profile index: 0
        { "command": null, "keys": "ctrl+shift+2" },    // New tab, profile index: 1
        { "command": null, "keys": "ctrl+shift+3" },    // New tab, profile index: 2
        { "command": null, "keys": "ctrl+shift+4" },    // New tab, profile index: 3
        { "command": null, "keys": "ctrl+shift+5" },    // New tab, profile index: 4
        { "command": null, "keys": "ctrl+shift+6" },    // New tab, profile index: 5
        { "command": null, "keys": "ctrl+shift+7" },    // New tab, profile index: 6
        { "command": null, "keys": "ctrl+shift+8" },    // New tab, profile index: 7
        { "command": null, "keys": "ctrl+shift+9" },    // New tab, profile index: 8
        { "command": null, "keys": "ctrl+alt+1" },      // Switch to tab, index: 0
        { "command": null, "keys": "ctrl+alt+2" },      // Switch to tab, index: 1
        { "command": null, "keys": "ctrl+alt+3" },      // Switch to tab, index: 2
        { "command": null, "keys": "ctrl+alt+4" },      // Switch to tab, index: 3
        { "command": null, "keys": "ctrl+alt+5" },      // Switch to tab, index: 4
        { "command": null, "keys": "ctrl+alt+6" },      // Switch to tab, index: 5
        { "command": null, "keys": "ctrl+alt+7" },      // Switch to tab, index: 6
        { "command": null, "keys": "ctrl+alt+8" },      // Switch to tab, index: 7
        { "command": null, "keys": "ctrl+alt+9" },      // Switch to tab, index: 8
        { "command": null, "keys": "win+sc(41)" },      // Summon Quake window
        // To learn more about selection, visit https://aka.ms/terminal-selection
        // Press Alt+Shift+F to open the search box
        {
            "command": "find",
            "keys": "alt+shift+f"
        },
        // Show or hide tab bar
        {
            "command": "toggleFocusMode",
            "keys": "alt+shift+t"
        },
        // Toggle command palette
        {
            "command": "commandPalette",
            "keys": "alt+shift+space"
        },
        // Switch to tab
        { "command": { "action": "switchToTab", "index": 0 }, "keys": "alt+shift+1" },
        { "command": { "action": "switchToTab", "index": 1 }, "keys": "alt+shift+2" },
        { "command": { "action": "switchToTab", "index": 2 }, "keys": "alt+shift+3" },
        { "command": { "action": "switchToTab", "index": 3 }, "keys": "alt+shift+4" },
        { "command": { "action": "switchToTab", "index": 4 }, "keys": "alt+shift+5" },
        { "command": { "action": "switchToTab", "index": 5 }, "keys": "alt+shift+6" },
        { "command": { "action": "switchToTab", "index": 6 }, "keys": "alt+shift+7" },
        { "command": { "action": "switchToTab", "index": 7 }, "keys": "alt+shift+8" },
        { "command": { "action": "switchToTab", "index": 8 }, "keys": "alt+shift+9" },
        // Press Alt+Shift+D to open a new pane.
        // - "split": "auto" makes this pane open in the direction that provides the most surface area.
        // - "splitMode": "duplicate" makes the new pane use the focused pane's profile.
        // To learn more about panes, visit https://aka.ms/terminal-panes
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        // tmux-like keybindings
        // Reference: https://docs.microsoft.com/en-us/windows/terminal/customize-settings/actions
        {
            "command":
            {
                "action": "splitPane",
                "split": "horizontal",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+_"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "vertical",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+|"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "horizontal",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+s"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "vertical",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+v"
        },
        {
            "command": "closePane",
            "keys": "alt+shift+x"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+shift+h"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+shift+j"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+shift+k"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+shift+l"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "alt+shift+ctrl+h"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "alt+shift+ctrl+j"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "alt+shift+ctrl+k"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "alt+shift+ctrl+l"
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+shift+z"
        },
        {
            "command": "duplicateTab",
            "keys": "alt+shift+c"
        },
        {
            "command": "nextTab",
            "keys": "alt+shift+n"
        },
        {
            "command": "prevTab",
            "keys": "alt+shift+p"
        },
        // Scroll up and down
        {
            "command": "scrollUpPage",
            "keys": "shift+pageup"
        },
        {
            "command": "scrollDownPage",
            "keys": "shift+pagedown"
        }
    ],
    "defaultProfile": "{bfb3dd1d-ccbc-4fc8-8c09-c6d7199ec0ba}",
    // Prevent dynamic profiles from being generated
    "disabledProfileSources":
    [
        "Windows.Terminal.Azure"
    ],
    "profiles":
    {
        "defaults":
        {
            "cursorShape": "underscore",
            //"fontFace": "DejaVuSansMono NF",
            "fontFace": "Hack NF",
            //"fontFace": "InconsolataGo NF",
            //"fontFace": "mononoki NF",
            //"fontFace": "SauceCodePro NF",
            //"fontFace": "UbuntuMono NF",
            "fontSize": 9,
            "scrollbarState": "hidden",
            "useAcrylic": true,
            "acrylicOpacity": 0.9
        },
        "list":
        [
            {
                "name": "PowerShell Core",
                "guid": "{bfb3dd1d-ccbc-4fc8-8c09-c6d7199ec0ba}",
                "commandline": "pwsh.exe -NoLogo",
                "startingDirectory": "%USERPROFILE%",
                "colorScheme": "Tokyo Night Storm",
                "icon": "%SCOOP%/apps/pwsh/current/assets/Square44x44Logo.png",
                "hidden": false
            },
            {
                "name": "Miniforge Powershell Prompt",
                "guid": "{9e2d3ba6-3809-423e-a85a-24c349c9e55a}",
                "commandline": "pwsh.exe -ExecutionPolicy ByPass -NoExit -Command \"& '%CONDA_HOME%/shell/condabin/conda-hook.ps1' ; conda activate '%CONDA_HOME%' \"",
                "startingDirectory": "%USERPROFILE%/development",
                "colorScheme": "DjangoRebornAgain",
                "icon": "%CONDA_HOME%/Lib/site-packages/conda/shell/conda_icon.ico",
                "hidden": false
            },
            {
                "name": "Windows PowerShell",
                "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
                "commandline": "powershell.exe",
                "colorScheme": "Campbell Powershell",
                "hidden": false
            },
            {
                "name": "Command Prompt",
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
                "commandline": "cmd.exe",
                "colorScheme": "Campbell",
                "hidden": false
            },
            {
                "name": "Ubuntu",
                "guid": "{2c4de342-38b7-51cf-b940-2309a097f518}",
                "commandline": "wsl.exe ~ -d Ubuntu",
                "source": "Windows.Terminal.Wsl",
                "colorScheme": "Gruvbox Dark",
                "hidden": false
            },
            {
                "name": "Debian",
                "guid": "{58ad8b0c-3ef8-5f4d-bc6f-13e4c00f2530}",
                "commandline": "wsl.exe ~ -d Debian",
                "source": "Windows.Terminal.Wsl",
                "colorScheme": "Solarized Dark",
                "hidden": false
            }
        ]
    },
