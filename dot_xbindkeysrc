# Volume controls #

"pamixer --decrease 5 --unmute"
  XF86AudioLowerVolume

"pamixer --increase 5 --unmute"
  XF86AudioRaiseVolume

"pamixer --toggle-mute"
  XF86AudioMute


# Media player controls #

"playerctl play-pause"
  XF86AudioPlay

"playerctl previous"
  XF86AudioPrev

"playerctl next"
  XF86AudioNext

